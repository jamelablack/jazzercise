<h1>Listing districts</h1>

<table>
  <tr>
    <th>Districtname</th>
    <th>Objectid</th>
    <th>Deleted</th>
    <th>Countryobjectid</th>
    <th>Districtmanagerobjectid</th>
    <th>Stateobjectid</th>
    <th>Districtcode</th>
    <th>Commissionpct</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @districts.each do |district| %>
  <tr>
    <td><%= district.DistrictName %></td>
    <td><%= district.ObjectID %></td>
    <td><%= district.Deleted %></td>
    <td><%= district.CountryObjectID %></td>
    <td><%= district.DistrictManagerObjectID %></td>
    <td><%= district.StateObjectID %></td>
    <td><%= district.DistrictCode %></td>
    <td><%= district.CommissionPct %></td>
    <td><%= link_to 'Show', district %></td>
    <td><%= link_to 'Edit', edit_district_path(district) %></td>
    <td><%= link_to 'Destroy', district, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New District', new_district_path %>
